{
  "name": "bycombo_app",
  "version": "1.0.0",
  "description": "Website for BYCombo.",
  "main": "index.js",
  "scripts": {
    "clean": "gatsby clean",
    "build:staging": "gatsby build",
    "build:production": "BUILD=production gatsby build",
    "deploy": "gatsby-plugin-s3 deploy --yes",
    "start": "npm run clean && DESTINATION=local BUILD=runtime gatsby develop --port 1111",
    "serve": "gatsby serve --port 1111",
    "stage": "npm run clean && DESTINATION=local npm run build:staging && npm run serve",
    "release:candidate": "npm run clean && DESTINATION=candidate npm run build:staging && npm run deploy",
    "release:delivery": "npm run clean && DESTINATION=delivery npm run build:production && npm run deploy",
    "release:production": "npm run clean && DESTINATION=production npm run build:production && npm run deploy",
    "lint": "eslint --ext js,jsx src",
    "lint:fix": "eslint --ext js,jsx --fix src"
  },
  "author": "BYCombo",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bennettslin/bycombo_app.git"
  },
  "bugs": {
    "url": "https://github.com/bennettslin/bycombo_app/issues"
  },
  "homepage": "https://www.bycombo.com",
  "dependencies": {
    "bowser": "^2.11.0",
    "classnames": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-inlinesvg": "^2.3.0",
    "react-markdown": "^6.0.3",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-tooltip": "^4.2.21",
    "redux": "^4.1.1",
    "remove-markdown": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "date-fns": "^2.23.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-react": "^7.24.0",
    "esm": "^3.2.25",
    "gatsby": "^3.12.0",
    "gatsby-plugin-google-gtag": "^3.12.0",
    "gatsby-plugin-manifest": "^3.12.0",
    "gatsby-plugin-react-helmet": "^4.12.0",
    "gatsby-plugin-remove-trailing-slashes": "^3.12.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sass": "^4.12.0",
    "jsdom": "^16.7.0",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.38.0",
    "webpack": "^5.51.1"
  }
}
